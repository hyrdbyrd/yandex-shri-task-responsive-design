{
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": false,
        "indent": [true, "spaces", 4],
        "linebreak-style": [true, "LF"],
        "trailing-comma": false,
        "quotemark": [true, "single"],
        "semicolon": [true, "always"],
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "curly": false,
        "no-unused-expression": false
    },
}
